  //criando lista a ser alterada
  List <String> nomeCompleto = ['Yasmin'];
  print (nomeCompleto);
  
  //adicionando o último sobrenome 
  nomeCompleto.add('Silva');
  print(nomeCompleto);
  
  //adicionando o sobrenome numa posição especifica
  nomeCompleto.insert(1, 'Guedes');
  print(nomeCompleto);
  
  //adicionando um nome pra remover
  nomeCompleto.add('Oliveira');
  print(nomeCompleto);
  
  //removendo nome errado
  nomeCompleto.removeAt(3);
  print(nomeCompleto);
  
  //verificando se oliveira ainda está na lista (false)
  nomeCompleto.contains('Oliveira');
  print(nomeCompleto.contains('Oliveira'));
  
  //verificando quantos elementos temos na lista
  print(nomeCompleto.length);
  
  //verificando qual o último elemento 
  print(nomeCompleto[nomeCompleto.length-1]);
  
  //verificando qual o 1° elemento 
  print(nomeCompleto[0]);
  
  //criando lista de key e values da lista (key se trata do n° do elemento, e value o elemento em sí)
int keys =0;
//var para add os valores da key

  //forEach conta os elementos da lista começando por 0, os atribui a string values e as apresenta na tela contando +1 cada vez que o forEach conta um elemento
nomeCompleto.forEach( (String values){
  
  print( '$keys $values');
  keys++;
  
});
}
operaçoes aritimeticas e lógicas
void main() {
  String resposta;
  resposta = 'O resultado da operação é:';
  
  int valor1 = 5;
  int valor2= 7;
  
  int resultado = valor1 + valor2;
  print ('$resposta $resultado');
  
  int n1 = 75;
  int n2 = 3;
  
  int result = n1-n2;
  
  print ('$resposta $result');
  
  //
  
  String valor;
  valor = 'O total da compra é:';
  
  //números não inteiros devem ser usados com . e não ,   
  double preco = 5.99;
  int quantidade = 17;
  
  double pagar = preco*quantidade;
  print ('$valor $pagar');
  
  
  //treinando lógica e condicionais 
  double money = 98.50;
  /*if (money >= pagar == true ) {
    print ('A compra foi aprovada');
  }
  else {
    print ('A compra foi recusada');
  }*/
  
  int senha; 
    senha = 1234;
  
  int password;
    password = 12345;
  
  if (senha == password || money >= pagar) {
    print ('Senha incorreta, saldo suficiente ');
  }
  else {
    print ('Senha correta! compra aprovada.');
  }
  
  if(preco is double) {
    print ('Você está comprando uma touca de cetim!');
  };
}

//lista para objetos
  //util para grandes listas, com muitas categorias, possive para o game 
  List <Cadastro> cadastro = [Cadastro('Yasmin',48913265818,06365653,11994742491), 
                              Cadastro('Eliana' ,28894045870, 06325984,11993519300),
                             Cadastro('Anderson', 26177073859, 06852395, 11998563200)];
  cadastro.forEach((Cadastro cadastro){
    print(cadastro.nome);
     print(cadastro.cpf);
     print(cadastro.cep);
     print(cadastro.telefone);
  });
  
}

class Cadastro {
  String nome = '';
  int cpf;
  int cep;
  int telefone;
  
  Cadastro(this.nome, this.cpf, this.cep, this.telefone);
